{
    "name": "chgst/chgst-bundle",
    "description": "Symfony bundle for chgst",
    "type": "library",
    "require": {
        "php": "^8.0",
        "symfony/framework-bundle": "~4",
        "symfony/security": "~4",
        "chgst/chgst":"^0.2.1"
    },
    "require-dev": {
        "phpspec/phpspec": "^7.0.1",
        "pdepend/pdepend": "^2.9.1",
        "phploc/phploc": "^7.0.2",
        "phpmd/phpmd": "^2.9.1",
        "sebastian/phpcpd": "^6.0.3",
        "symfony/templating": "~5",
        "friends-of-phpspec/phpspec-code-coverage": "^6",
        "php-coveralls/php-coveralls": "^v2.4.3",
        "symfony/expression-language": "^v5.2.4"
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Changeset\\ChangesetBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "spec/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "8.0"
        }
    }
}
