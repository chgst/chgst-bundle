{
    "name": "chgst/chgst-bundle",
    "description": "Symfony bundle for chgst",
    "type": "library",
    "require": {
        "php": "^7.2",
        "symfony/framework-bundle": "~2.3||~3||~4",
        "symfony/security": "~2.3||~3||~4",
        "chgst/chgst":"^0.2.1"
    },
    "require-dev": {
        "phpspec/phpspec": "^6",
        "pdepend/pdepend": "^2.5",
        "phploc/phploc": "^4.0",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^4.0",
        "symfony/templating": "~2.3||~3||~4",
        "friends-of-phpspec/phpspec-code-coverage": "^4.3",
        "php-coveralls/php-coveralls": "^2.2",
        "symfony/expression-language": "~2.3||~3||~4"
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Changeset\\ChangesetBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "spec/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.2"
        }
    }
}
